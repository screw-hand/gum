      "request": "launch",
      "name": "gum set user1 --name lisi --email lisi@gmail.com",
      "runtimeExecutable": "gum",
      "runtimeArgs": ["set", "user1", "--name", "lisi", "--email", "lisi@gmail.com"],
      "skipFiles": ["<node_internals>/**"]
